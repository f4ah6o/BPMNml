---
title: BPMN with Pools and Lanes
---

bpmn-beta
    pool CustomerPool {
        lane CustomerLane {
            event Start1 <<start>>
            task SubmitOrder
            event End1 <<end>>

            Start1 --> SubmitOrder
            SubmitOrder --> End1
        }
    }

    pool CompanyPool {
        lane SalesLane {
            event Start2 <<start>>
            task ProcessOrder
            task CheckInventory
            event End2 <<end>>

            Start2 --> ProcessOrder
            ProcessOrder --> CheckInventory
            CheckInventory --> End2
        }

        lane WarehouseLane {
            event Start3 <<start>>
            task PackItems
            task ShipOrder
            event End3 <<end>>

            Start3 --> PackItems
            PackItems --> ShipOrder
            ShipOrder --> End3
        }
    }
